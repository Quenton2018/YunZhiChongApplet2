page{background: #efeff4;}

.container {display: flex;flex-direction: column;align-items: center;justify-content: space-between;
  padding: 200rpx 0;box-sizing: border-box;}
.custom{width: 100%;position: fixed;top: 0;left: 0;height: 45px;line-height: 45px;background: #ff7426;z-index: 999;
  text-align: center;}
.top-title{width: 100%;padding-left:80px;padding-right:80px;box-sizing: border-box;overflow: hidden;overflow: hidden;white-space: nowrap; 
  text-overflow: ellipsis;color: #fff}
.custom text{color: #fff;font-size: 38rpx;font-weight: 500;max-width: 280rpx;}
.empty_custom{height: 45px;width: 100%;}
.custom image{width: 16px;height: 18px;vertical-align: -2px}

.yzc-main{width: 100%;height: 150px;margin-top: -5px;background: #ff7426;
	  padding: 0 10px;box-sizing: border-box;}
.myContent{width: 100%;box-sizing: border-box;padding: 10px;margin-top: -140px}
.h2-title{line-height: 40px;padding: 0 8px;text-align:left;font-weight:600;color:#454545;letter-spacing:1px;margin:0;
  font-size: 14px}
.h2-title text{font-size: 16px;vertical-align: top;font-weight: 700}
.last,.prev{width: 50px;height: 50px;border-radius: 80%;background: rgba(99, 99, 99, 0.3);position: absolute;top: 7px;
  line-height: 45px;color: #fff;font-size: 30px;box-sizing: border-box}
.last{left: -37px;text-align: right;padding-right: 3px}
.prev{right: -37px;text-align: left;padding-left: 3px}

swiper{previous-margin:20px;padding: 10px 30px;box-sizing: border-box;height: 75px}
swiper swiper-item{width: 80%;margin: 0 auto;text-align: center}
.section_title{background: #ccc;width: 44px;height: 44px;border-radius: 50%;text-align: center;color: #fff;margin: 0 auto;           line-height: 44px;font-size: 20px;box-shadow: 0px 1px 2px 1px rgb(209, 207, 207)}
.section_title.active{background:#F17746;color:#fff;}
.hr{height: 1px;background: rgba(99, 99, 99, 0.1)}
/* 插座 */
.socke-box{width: 100%;}
.socke-box{display: flex;flex-flow: row wrap;align-content: flex-start;justify-content: space-between;overflow: hidden;padding: 0     6px;box-sizing: border-box;padding-bottom: 20px}
.socket-list{flex: 0 0 14.3%;width: 45px;height: 45px;border-radius: 5px;background:#F2F2F2;text-align: center;line-height: 45px;
  color:#515151;margin-bottom: 7px;margin: 0 auto;margin-top: 8px;box-shadow:1px 1px 2px #888888;font-size: 20px}
.socket-list.active{background:#F17746;color:#fff;}
.charing{background:#EDEDED;color:#fff;box-shadow:1px 1px 2px #888888;font-size: 20px;border-radius: 5px;}
/* 时长 */
.sel-time{width: 100%;padding: 0 10px;box-sizing: border-box;}
.time-content{width: 100%;background: #fff;border-radius: 10px;box-sizing: border-box;}
.time-list{display: flex;flex-flow: row wrap;align-content: flex-start;justify-content: space-between;overflow: hidden;
  padding: 0 10px 20px;}
.time{width: 32%;height: 40px;border-radius: 20px;background:#F2F2F2;text-align: center;line-height: 40px;
  color:#515151;margin-top: 9px;box-shadow:1px 1px 2px #888888;font-size: 16px}
.time-list::after{content: "";flex: 0 0 32%;}
.time.active{background:#F17746;color:#fff;}
.null{width: 100%;height: 70px}
/* 收费说明 */
.state{width: 100%;box-sizing: border-box;padding: 0 20px 60px;margin-top: -45px}
.state .state-top{font-size: 14px}
.toll-wrap{width: 100%;padding: 0 10px;box-sizing: border-box;display: flex;flex-flow: row wrap;justify-content: space-between;          overflow: hidden}
.state-list{width:50%;}
.state-list text{font-size: 12px}
/* fotter */
.fotter{width: 100%;position: fixed;bottom: 0;left: 0}
.fotter button{width: 100%;height: 50px;background: linear-gradient(to right, #FF7546 , #F08200);border-radius: 0;
  color: #fff;line-height: 50px}
.total{position: absolute;bottom: 12px;left: 20px;z-index: 999;color: #fff;font-size: 16px}
/* 下单进度 */
.progressBoxWrap{position:fixed;top:0;width:100%;height:100%;bottom:0;background:rgba(0,0,0,.3);z-index:9999;overflow:hidden;display: block;}
.progressBox{background:#fff;width:300px;height:400px;position: absolute;top: 0;right: 0;bottom: 0;left: 0;margin: auto;    border-radius:10px;overflow:hidden;}
.progressBoxTitle{text-align:center;background:#EF7F35;padding:10px;color:#fff;}
.stepBox{padding:20px;}
.stepBox .stepItem{width:160px;margin:0 auto;padding:10px 0;color:#666;font-size:16px;position: relative}
.stepBox .stepItem image{display: inline-block;width: 18px;height: 18px;vertical-align: -3px;margin-right: 10px;
  position: relative;z-index: 999}
.stepItem::after{content: "";width: 2px;height: 30px;background: rgb(171, 175, 169);position: absolute;left: 8px;top: 29px;z-index: 800}
.stepItem:last-child::after{height: 0}
.one-true{color: rgb(13, 127, 41)}

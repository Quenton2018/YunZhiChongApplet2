page{width: 100%;height: 100%;background: #f2f2f2;overflow: hidden}
.box{width: 100%;height: 100%;background: #f2f2f2;overflow: hidden;text-align: center}
.box .iphonex{width: 100%;height: 90%;}
.box .iphone{width: 90%;height: 94%;margin: 0 auto;}

.btn image,.btnx image{width: 16px;height: 16px;position: absolute;bottom: 15px;left: 33%;z-index: 1000}
.btnx{width: 100%;position: fixed;padding: 0 20px;bottom: 50px;box-sizing: border-box;z-index: 999}
.btn{width: 100%;position: fixed;padding: 0 20px;bottom: 30px;box-sizing: border-box;z-index: 999}
.btn-submit{border-radius: 50px;background:linear-gradient(86deg,rgba(255,156,44,1),rgba(255,109,34,1));
  text-align: center;color: #fff;}

